  function tryOpenFromHash() {
    var hash = window.location.hash;
    if (!hash) { return; }

    var id = hash.replace('#', '');
    if (!id) { return; }

    var $el = $('#' + id);
    if (!$el.length) { return; }

    // 1) If the ID is directly on the tab title element
    if ($el.is('.e-n-tab-title, .elementor-tab-title, [role="tab"]')) {
      $el.get(0).click();
      return;
    }

    // 2) If the ID is on something inside the tab title
    var $closestTitle = $el.closest('.e-n-tab-title, .elementor-tab-title, [role="tab"]');
    if ($closestTitle.length) {
      $closestTitle.get(0).click();
      return;
    }

    // 3) If the ID is on the tab CONTENT panel, activate the title that controls it
    // (Elementor uses aria-controls to link title -> content)
    var $controller = $('[aria-controls="' + id + '"]').first();
    if ($controller.length) {
      $controller.get(0).click();
      return;
    }
  }

  function openTabFromHashWithRetries() {
    var attempts = 0;
    var maxAttempts = 20;      // ~4 seconds total
    var intervalMs = 200;

    var timer = setInterval(function () {
      attempts += 1;
      tryOpenFromHash();

      // If Elementor has opened it, the title usually becomes aria-selected="true"
      var hash = window.location.hash;
      var id = hash ? hash.replace('#', '') : '';
      if (id) {
        var $title = $('#' + id + '[aria-selected="true"], [aria-controls="' + id + '"][aria-selected="true"]');
        if ($title.length) {
          clearInterval(timer);
        }
      }

      if (attempts >= maxAttempts) {
        clearInterval(timer);
      }
    }, intervalMs);
  }

  $(window).on('load', openTabFromHashWithRetries);
  $(window).on('hashchange', openTabFromHashWithRetries);

